{
    
    "extends": ["airbnb","prettier","plugin:node/recommended"],
    "plugins": ["prettier"],
    "rules": {
        "no-unused-vars":"warn",
        "prettier/prettier": "error",
        "no-console":"off",
        "func-names":"off",
        "object-shorthand":"off",
        "no-plusplus":"off",
        "no-param-reassign":"off"
    },
    "env":{
        "browser": true
    }
}
